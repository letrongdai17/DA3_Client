{
  "name": "fresher-client",
  "version": "0.0.1",
  "description": "ocrClient",
  "main": "index.html",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.dev.js",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "server": "http-server -g",
    "test": "mocha  --require  babel-register",
    "tests": "mocha  --require babel-register --recursive './src/js/**/__tests__/*.*'",
    "eslint": "eslint './src/js/**/*.*'",
    "stylelint": "stylelint './src/css/*.*'",
    "css": "node-sass ./src/css/style.scss ./public/css/style.css",
    "watch-css": "node-sass -w ./src/css/style.scss ./public/css/style.css",
    "webpack": "webpack --config=webpack.dev.js",
    "build:dev": "node-sass ./src/css/style.scss ./public/css/style.css && webpack --config=webpack.dev.js",
    "build:dev:watch": "node-sass ./src/css/style.scss ./public/css/style.css && webpack --config=webpack.dev.js --watch --progress",
    "build:test": "node-sass ./src/css/style.scss ./public/css/style.css --output-style compressed && webpack --config=webpack.test.js",
    "build:staging": "node-sass ./src/css/style.scss ./public/css/style.css --output-style compressed && webpack --config=webpack.staging.js",
    "build:production": "node-sass ./src/css/style.scss ./public/css/style.css --output-style compressed && webpack --config=webpack.prod.js"
  },
  "author": "@ocr",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.7.1",
    "chart.js": "^2.8.0",
    "chartjs-plugin-labels": "^1.1.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.12",
    "ionicons": "^4.6.1",
    "japan-postal-code": "^0.2.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-chartjs-2": "^2.7.6",
    "react-datepicker": "^2.3.0",
    "react-dom": "^16.6.1",
    "react-redux": "^5.0.7",
    "react-rnd": "^9.1.2",
    "react-router-dom": "^4.3.1",
    "react-s-alert": "^1.4.1",
    "react-select": "^2.4.3",
    "react-stripe-elements": "^3.0.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.4.9",
    "validator": "^10.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.0-alpha.23",
    "@storybook/addon-notes": "^4.0.0-alpha.23",
    "@storybook/addon-viewport": "^4.0.0-alpha.23",
    "@storybook/addons": "^4.0.0-alpha.23",
    "@storybook/react": "^4.0.0-alpha.23",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jsdom": "^16.5.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.2",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  }
}
